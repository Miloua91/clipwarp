{
  "expo": {
    "name": "ClipWarp",
    "slug": "clipwarp",
    "scheme": "clipwarp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "notification": {
      "icon": "./assets/notifIcon.png",
      "color": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mokhincode.ClipWarp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mokhincode.ClipWarp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      ["expo-asset"],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notifIcon.png",
          "color": "#000000",
          "defaultChannel": "default"
        }
      ],
      "expo-font",
      "expo-share-intent",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "e1f8c8e3-5c2f-406c-8b49-949789ab16e6"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e1f8c8e3-5c2f-406c-8b49-949789ab16e6"
    }
  }
}
